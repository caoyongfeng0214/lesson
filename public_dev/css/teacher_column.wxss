.displayFlex {
  display: flex;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
}
.notselect {
  user-select: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  -o-user-select: none;
}
.container {
  background-color: #fff;
}
.open-message {
  color: #f75858;
  font-size: 16px;
  font-family: 'ArialMT';
}
.display-none {
  display: none;
}
.display-block {
  display: block;
}
.activation {
  padding: 5px 25px 15px;
  background-color: rgba(64, 158, 254, 0.1);
  text-align: center;
  border-bottom: 1px solid #d2d2d2;
}
.activation .activation-content {
  color: #333;
  font-size: 17px;
  font-weight: bold;
}
.activation .activation-content p {
  margin: 8px 0;
}
.activation .activation-content .notice {
  color: #FF414A;
}
.activation .activation-content .el-input {
  width: 322px;
  margin-bottom: 14px;
}
.activation .activation-content .el-input .el-input__inner {
  border: 1px solid #409EFE;
}
.activation .activation-dismiss {
  position: absolute;
  right: 25px;
  top: 8px;
  font-size: 14px;
  color: #6a6a6a;
  text-decoration: underline;
  cursor: pointer;
}
.get_detail {
  padding: 0 40px;
}
.teacher_title {
  color: #333;
  font-size: 20px;
  margin: 20px 0;
  font-family: 'ArialMT';
}
.user-authority > div {
  width: 48%;
  margin-bottom: 5px;
  border: 1px solid #eee;
  background: #ffffff;
  box-shadow: 0 0px 11px rgba(0, 0, 0, 0.08);
  color: #999;
}
.user-authority > div h3 {
  margin: 0 0 10px;
  font-size: 26px;
  color: #333;
  font-weight: 500;
  line-height: 1;
}
.user-authority > div .head {
  padding: 33px 10px;
  background: #f7f7f7;
  text-align: center;
}
.user-authority > div.edu-ins .head {
  background: url(/imgs/main/buy_vip_bg.png) no-repeat center center;
  background-size: cover;
}
.user-authority > div.edu-ins .head h3 {
  color: #FEFFB3;
}
.user-authority > div .body-content {
  padding: 8px 0 40px;
}
.user-authority > div .body-content ul {
  padding: 0 10px 0 25px;
}
.user-authority > div .body-content li {
  padding-left: 30px;
  margin-top: 20px;
  line-height: 24px;
  color: #818181;
  font-size: 16px;
  list-style: none;
  position: relative;
  font-family: 'Microsoft YaHei';
  font-weight: lighter;
}
.user-authority > div .body-content li.have {
  color: #333;
}
.user-authority > div .body-content li.have::before {
  content: "\E639";
  position: absolute;
  left: 0;
  font-size: 20px;
  color: #10C55B;
  font-family: element-icons !important;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  vertical-align: baseline;
  display: inline-block;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.teacherWrapper {
  max-width: 1200px;
  height: auto;
  margin: 0 auto;
  background-color: #fff;
}
.teacher-record .user-info {
  align-items: flex-end;
  -webkit-align-items: flex-end;
  -moz-align-items: flex-end;
  -ms-align-items: flex-end;
  -o-align-items: flex-end;
  padding: 30px 50px;
  border-bottom: 1px solid #d2d2d2;
}
.teacher-record .user-info .user-desc .el-row--flex {
  align-items: center;
  -webkit-align-items: center;
  -moz-align-items: center;
  -ms-align-items: center;
  -o-align-items: center;
}
.teacher-record .user-info .user-desc .portrait {
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  display: inline-block;
  background-color: #eee;
  width: 60px;
  height: 60px;
  object-fit: cover;
  -o-object-fit: cover;
}
.teacher-record .user-info .user-desc strong {
  color: #333;
  font-size: 20px;
  margin-left: 15px;
}
.teacher-record .user-info .teacher-summary .el-col {
  border-right: 1px solid #B5B5B5;
  text-align: center;
}
.teacher-record .user-info .teacher-summary .el-col .title {
  color: #6a6a6a;
  font-size: 14px;
}
.teacher-record .user-info .teacher-summary .el-col .lines {
  color: #333;
  font-size: 16px;
  font-weight: bold;
  margin-top: 15px;
}
.teacher-record .user-info .teacher-summary .el-col .lines span {
  font-size: 18px;
  margin: 0 5px;
  color: #ff414a;
}
.teacher-record .user-info .teacher-summary .el-col:last-child {
  border: none;
}
.no-data {
  padding: 30px 92px;
}
.no-data-content {
  padding-top: 195px;
  background: url(/imgs/search.png) no-repeat top center;
  background-size: 200px;
  font-size: 24px;
  line-height: 30px;
  color: #b8b8b8;
  text-align: center;
}
.have-taught .sort-btn {
  display: flex;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  padding: 10px;
  font-size: 16px;
  color: #333;
  cursor: pointer;
  user-select: none;
  border-top: 1px solid #E5E5E5;
}
.have-taught .sort-btn .sort-icon {
  padding-right: 5px;
}
.have-taught .sort-btn .sort-icon > i {
  display: block;
  line-height: 8px;
  color: #CCC;
}
.have-taught .sort-btn .sort-icon > i.active {
  color: #49A5F8;
}
.have-taught .have-item {
  border: 1px solid #E5E5E5;
  margin-bottom: 20px;
  font-family: 'Microsoft YaHei';
}
.have-taught .have-item .time {
  padding: 8px 15px;
  color: #181818;
  font-size: 18px;
  border-bottom: 1px solid #E5E5E5;
}
.have-taught .have-item .layout-box {
  padding: 15px 20px 15px 10px;
}
.have-taught .have-item .layout-box .item-cover {
  width: 250px;
  height: 146px;
}
.have-taught .have-item .layout-box .item-cover a {
  width: 100%;
  height: 100%;
  display: inline-block;
}
.have-taught .have-item .layout-box .cover {
  width: 100%;
  height: 100%;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
}
.have-taught .have-item .layout-box .content {
  flex: 1;
  margin: 0 20px;
}
.have-taught .have-item .layout-box .content .title,
.have-taught .have-item .layout-box .content .duration,
.have-taught .have-item .layout-box .content .goals {
  font-size: 14px;
  color: #333;
  font-weight: bold;
  margin: 12px 0;
  display: block;
}
.have-taught .have-item .layout-box .content .title span,
.have-taught .have-item .layout-box .content .duration span,
.have-taught .have-item .layout-box .content .goals span,
.have-taught .have-item .layout-box .content .title ul,
.have-taught .have-item .layout-box .content .duration ul,
.have-taught .have-item .layout-box .content .goals ul {
  font-weight: lighter;
}
.have-taught .have-item .layout-box .content .title:hover {
  text-decoration: underline;
  color: #49A5F8;
}
.have-taught .have-item .layout-box .content .package-title > .title {
  font-size: 18px;
  margin: 0;
}
.have-taught .have-item .layout-box .content .goals > ul {
  padding-left: 18px;
}
.have-taught .have-item .layout-box .content .goals > ul li {
  margin-top: 8px;
  color: #333;
  word-break: break-all;
  word-wrap: break-word;
  white-space: pre-line;
}
.have-taught .have-item .layout-box .content .foot {
  width: 90%;
  display: flex;
  display: -webkit-flex;
  display: -moz-flex;
  display: -ms-flex;
  display: -o-flex;
  align-items: center;
  -webkit-align-items: center;
  -moz-align-items: center;
  -ms-align-items: center;
  -o-align-items: center;
  justify-content: space-between;
  -webkit-justify-content: space-between;
  -moz-justify-content: space-between;
  -ms-justify-content: space-between;
  -o-justify-content: space-between;
  flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -moz-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  -o-flex-wrap: wrap;
  color: #111;
}
.have-taught .have-item .layout-box .content .foot .scores-info {
  margin: 10px 0;
}
.have-taught .have-item .layout-box .content .foot span {
  margin-right: 15px;
}
.have-taught .have-item .layout-box .content .foot span em {
  color: #FF414A;
}
@media (max-width: 768px) {
  .container {
    padding: 0 10px;
  }
  .get_detail .user-authority {
    flex-direction: column;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
  }
  .get_detail .user-authority > div {
    width: 100%;
  }
}
@media (max-width: 415px) {
  .teacher-record .user-info {
    padding: 30px 15px;
    flex-direction: column;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
  }
  .teacher-record .user-info .teacher-summary {
    margin-top: 10px;
  }
  .teacher-record .have-taught .layout-box {
    flex-direction: column;
    -webkit-flex-direction: column;
    -moz-flex-direction: column;
    -ms-flex-direction: column;
    -o-flex-direction: column;
  }
  .teacher-record .have-taught .layout-box .item-cover {
    margin: 0 auto;
  }
  .teacher-record .have-taught .layout-box .content {
    margin-top: 20px;
  }
}
